find_doubling <- function(country.table, days, row.use) {
count <- 1
row.start <- row.use - 1
numrows_lim <- row.use - 10
for (i in row.start:numrows_lim) {
is.double <- as.number(country.table[, days][row.use] / country.table[, days][i])
if ( is.double >= 2) {
return(count)
}
else {
count <- count + 1
}
}
return(count)
}
